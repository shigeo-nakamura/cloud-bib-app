<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_return">

    <fragment
        android:id="@+id/navigation_borrow"
        android:name="com.cloudbib.client.ui.borrow.BorrowFragment"
        android:label="@string/title_borrow"
        tools:layout="@layout/fragment_borrow" />

    <fragment
        android:id="@+id/navigation_return"
        android:name="com.cloudbib.client.ui.return.ReturnFragment"
        android:label="@string/title_return"
        tools:layout="@layout/fragment_return" />

    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.cloudbib.client.ui.setting.SettingFragment"
        android:label="@string/title_setting"
        tools:layout="@layout/fragment_setting" />
</navigation>